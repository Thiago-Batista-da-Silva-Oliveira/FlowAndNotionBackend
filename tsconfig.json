{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./build",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "typeRoots": [
    "./src/@types",
    "./custom_typings",
    "./@types",
    "./node_modules/@types"
  ],
  "include": [
    "src/**/*"
, "prisma/seed.ts"  ]
}
